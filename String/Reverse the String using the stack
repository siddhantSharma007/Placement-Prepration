class Solution {
    
    public String reverse(String S){
        
         String str = "";
       Stack<Character> s = new Stack<>();
       for(int i = 0;i<S.length();i++)
       {
           s.push(S.charAt(i));
       }
        while(!s.isEmpty())
       {
           str+=s.pop();
       }
       return str;
   }

}
